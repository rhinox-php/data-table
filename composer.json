{
    "name": "rhinox/data-table",
    "license": "MIT",
    "require": {
        "symfony/http-foundation": ">3.0",
        "rhinox/input-data": "^0.9.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Rhino\\DataTable\\": "classes/"
        },
        "files": [
            "include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rhino\\DataTable\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.2",
        "fzaninotto/faker": "^1.9",
        "vimeo/psalm": "^3.12",
        "phpmd/phpmd": "^2.8",
        "brianium/paratest": "^4.1",
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
